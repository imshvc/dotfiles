# begin: msys2 exports

# handle: cmd.exe path -- see /usr/bin/cmd (MSYS2)
export COMSPEC=/c/Windows/System32/cmd.exe

# handle: program files (apps) directories
if [ "$APPS_DIR" = "" ]; then
  export APPS_DIR="/c/Program Files"
fi

if [ "$APPS_DIR_X86" = "" ]; then
  export APPS_DIR_X86="/c/Program Files (x86)"
fi

# end: msys2 exports
